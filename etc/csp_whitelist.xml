                <value id="landofcoder-com-src" type="host">landofcoder.com</value>
            </values>
        </policy>
        <policy id="object-src">
            <values>
                <value id="landofcoder-com-object" type="host">landofcoder.com</value>
            </values>
        </policy>
    </policies>
